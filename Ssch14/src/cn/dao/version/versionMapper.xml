<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.dao.version.versionMapper">
	<select id="getid" resultType="app_version">
		SELECT * FROM app_version WHERE appId = #{appId}
	</select>
	
	<insert id="add" parameterType="app_version">
		insert into app_version (versionNo,versionSize,publishStatus,versionInfo,apkLocPath) 
        values (#{versionNo},#{versionSize},#{publishStatus},#{versionInfo},#{apkLocPath})
	</insert>
</mapper>